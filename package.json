{
  "name": "jm-showroomer-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "// core": "----",
    "test": "npm run test:unit",
    "lint": "npx @redocly/cli lint docs/openapi.yaml",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest",
    "// daily": "----",
    "test:smoke": "NODE_ENV=test ./src/test/core/test_smoke.sh",
    "test:flows": "NODE_ENV=test ./src/test/flows/test_flows.sh",
    "test:notifications": "NODE_ENV=test ./src/test/flows/test_notifications_full.sh",
    "test:push": "NODE_OPTIONS=--experimental-vm-modules jest --watchman=false src/services/push/__tests__/pushService.test.js",
    "// full validation": "----",
    "test:full": "NODE_ENV=test bash ./src/test/run.sh all-full",
    "test:all": "npm run lint && npm run test:unit && npm run test:smoke && npm run test:flows && npm run test:notifications && npm run test:push && npm run test:full",
    "// production": "----",
    "test:prod-smoke": "NODE_ENV=prod bash ./src/test/prod/test_prod_readonly.sh",
    "// maintenance": "----",
    "test:cleanup": "NODE_ENV=test node scripts/cleanup_test_data.js",
    "test:cleanup:dry": "NODE_ENV=test node scripts/cleanup_test_data.js --dry-run",
    "firebase:migration": "bash scripts/firebase_project_migration.sh",
    "dev": "NODE_ENV=dev nodemon src/core/server.js",
    "start": "NODE_ENV=prod node src/core/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/echocode-app/jm-showroomer-back.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/echocode-app/jm-showroomer-back/issues"
  },
  "homepage": "https://github.com/echocode-app/jm-showroomer-back#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "joi": "^17.13.3",
    "libphonenumber-js": "^1.12.36",
    "lodash.isequal": "^4.5.0",
    "ngeohash": "^0.6.3",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3"
  }
}
